  LAPS=(a1+100)/100;
  screen("      Laps","       ",LAPS);
  for(int p = 0; p < players; p++){
      buttonPrev[p] = digitalRead(controlPins[p]);
  }
  while(!start) {
    delay(100);
    a2=analogRead(A7);
    if(abs(a1-a2)>50){
      a1=a2;
      if(LAPS!=(a2+100)/100){
        LAPS=(a2+100)/100;
        tone(PIN_AUDIO, 2637,50);
        screen("      Laps","       ",LAPS);
      }
    }
    
    for(int p = 0; p < players; p++){
      if(activePlr[p]){ 
        if(buttonPrev[p] != digitalRead(controlPins[p])){
          delay(50);
          start = 1;
          buttonPrev[p] = digitalRead(controlPins[p]);
        } 
      }
    }
  }
  noTone(PIN_AUDIO);
  startPlayback(roar, sizeof(roar));
  delay(2000);
}
